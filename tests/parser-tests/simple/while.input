require "ifj21"
function main() -- uzivatelska funkce bez parametru
	local a : integer = 5
	local vysl : integer = 0
	if a < 0 then
		write("Faktorial nelze spocitat\n")
	else
		vysl = 1
		while a > 0 do
			vysl = vysl * a a = a - 1 -- dva prikazy
		end
		write("Vysledek je: ", vysl, "\n")
	end
end
main()
