require "ifj21"

function foo(i : integer)
	local f : string = "ahoj"..("kek".."kok\n")
	if i > #f then
		write("BIG\n")
	else
		write("SMALL\n")
	end
	local a : integer = 5
	while i > 0 do
		if (i > a) then
			write("i is bigger\n")
			a = i -a
		else
			write("i is smaller\n")
			a = a - i
		end
	i = i - 1
	end
end


foo(5)
